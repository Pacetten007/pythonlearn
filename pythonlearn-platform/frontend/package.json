    "pyodide": "^0.24.1",
    "recharts": "^2.8.0",
    "lucide-vue-next": "^0.298.0",
    "@vueuse/core": "^10.7.0",
    "@vueuse/integrations": "^10.7.0",
    "date-fns": "^2.30.0",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "marked": "^11.1.1",
    "highlight.js": "^11.9.0",
    "sortablejs": "^1.15.1",
    "vue-toastification": "^2.0.0-rc.5",
    "@headlessui/vue": "^1.7.16",
    "chart.js": "^4.4.1",
